{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "require-jsdoc": "off",
        "no-unused-vars": "off",
        "no-undef":"error"
    },
    "ignorePatterns": ["*_pb.js"]
}
